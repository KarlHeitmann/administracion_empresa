<div class="text-xl font-medium text-black">Empleados</div>

<table class="table-fixed">
  <thead>
    <tr>
      <td class="w-1/8">Nombre</td>
      <td class="w-1/8">Foto</td>
      <td class="w-1/8">Email</td>
      <td class="w-1/8">Boss</td>
      <td class="w-1/8">Employees</td>
    </tr>
  </thead>
  <tbody>
    <% @employees.each do |employee| %>
      <%= render 'table_employee', employee: employee %>
    <% end %>
  </tbody>
</table>
